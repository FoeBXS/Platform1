}, 1);
setInterval(function(){
















introvalue = introvalue + 1;








if (introvalue >= 100){
introtick = 10000000000000;
endintro();
}
else if (introvalue >= 50){
if (runonce === false){
introtick = Math.floor(Math.random() * 150 + 1);








runonce = true;
}
}








}, introtick);
