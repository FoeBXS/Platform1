window.addEventListener('keydown', (event) => {
if (event.ctrlKey) {
if (sidemanuvarforaddeventlistener == 0){
sidemanuvarforaddeventlistener = 1;
sidemenuvar.classList.add("sidemenu-open");
}
else if (sidemanuvarforaddeventlistener == 1){
sidemenuvar.classList.remove("sidemenu-open");
sidemanuvarforaddeventlistener = 0;
}
}
});
