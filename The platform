let fullscreening = 1;
function fullscreengamblinggame(){








if (fullscreening == 1){
gamblinggameembedvar.style.width = "100" + '%';
gamblinggameembedvar.style.height = "100" + '%';
gamblinggameembedvar.style.left = "0" + '%';
gamblinggameembedvar.style.top = "0" + '%';
fullscreening = 2;
fullscreenIDgamblinggame = "Normal screen";
}
else if (fullscreening == 2){
gamblinggameembedvar.style.width = "50" + '%';
gamblinggameembedvar.style.height = "75" + '%';
gamblinggameembedvar.style.left = "25" + '%';
gamblinggameembedvar.style.top = "10" + '%';
fullscreening = 1;
fullscreenIDgamblinggame = "Full screen";
}
}
