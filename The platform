let updatelog = document.querySelector(".updatelog");
let updatelogBackground = document.querySelector(".updatelogBackground");
let gamespagevar = document.querySelector('.gamespage');
let gamblinggameembedvar = document.querySelector('.gamblinggameembed');
let gamblinggameclassvar = document.querySelector('.gamblinggameclass');
let investorrestclassvar = document.querySelector('.investorrestclass');
let investorrestembevar = document.querySelector('.investorrestembed');
let fightingsimclassvar = document.querySelector('.fightingsimclass');
let fightingsimembedvar = document.querySelector('.fightingsimembed');
let exterminationclassvar = document.querySelector('.exterminationclass');
let exterminationembedvar = document.querySelector('.exterminationembed');
let teacherexeclassvar = document.querySelector('.teacherexeclass');
let teacherexeembedvar = document.querySelector('.teacherexeembed');
let investorrest2classvar = document.querySelector('.investorrest2class');
let investorrest2embedvar = document.querySelector('.investorrest2embed');
let linkopenerclassvar = document.querySelector('.linkopenerclass');
let linkopenerembedvar = document.querySelector('.linkopenerembed');
let walkingsimclassvar = document.querySelector('.walkingsimclass');
let walkingsimembedvar = document.querySelector('.walkingsimembed');
let rpsclassvar = document.querySelector('.rpsclass');
let rpsembedvar = document.querySelector('.rpsembed');
let runonce = false;
let introloading;
let otherspagevar = document.querySelector('.otherspage');
let commandcenterbackgroundvar = document.querySelector('.commandcenterbackground');


const params = `scrollbars=no,resizable=no,status=no,location=no,toolbar=no,menubar=no,
width=20000,height=10000,left=-1000,top=-2000`;


function opencommandcenter(){
commandcenterbackgroundvar.classList.add("open");
}
function runthecommandcenter(){
commandcenterbackgroundvar.classList.remove("open");
let commandcentervar = document.getElementById("commandcenterID").value;
console.log(commandcentervar);
if (commandcentervar === "run.background < audio" || commandcentervar === "run.background<audio" || commandcentervar === "run.Background < Audio" || commandcentervar === "run.background < Audio" || commandcentervar === "run.Background < audio"){
//run background audio
document.getElementById('backgroundaudioID').play();
alert("Succesfull!");
}
else if (commandcentervar === "stop.background < audio" || commandcentervar === "stop.background<audio" || commandcentervar === "stop.Background < Audio" || commandcentervar === "stop.background < Audio" || commandcentervar === "stop.Background < audio"){
// stop background audio
document.getElementById('backgroundaudioID').pause();
alert("Succesfull!");
}
else if (commandcentervar === "close.self/deactivate < shutdown"){
alert("Succesfull!");
window.close();
}
else if (commandcentervar === "open.new < tab"){
alert("Succesfull!");
open("https://google.com");


}
else if (commandcentervar === "run.audio/audio.random.weird#audio < play-audio.randomaudio#1.00"){
document.getElementById('weirdauiod1ID').play();
alert("Succesfull!");
}
else if (commandcentervar === "open.new < window"){
window.open("https://google.com", "target", params);
alert("Succesfull!");
}
else if (commandcentervar === "open.game < GamblingGame" || commandcentervar === "open.game < gamblinggame" || commandcentervar === "open.game < Gambling game" || commandcentervar === "open.game < gambling game" || commandcentervar === "open.game < Gambling game"){
// open the game
GamblingGame();
}
else if (commandcentervar === "open.game < Invest or rest" || commandcentervar === "open.game < Investorrest" || commandcentervar === "open.game < investorrest" || commandcentervar === "open.game < invest or rest" || commandcentervar === "open.game < Invest Or Rest"){
// open the game
openinvestorrest();
}
else if (commandcentervar === "open.game < Fighting Simulator" || commandcentervar === "open.game < Fighting simulator" || commandcentervar === "open.game < fighting simulator" || commandcentervar === "open.game < fightingsimulator" || commandcentervar === "open.game < Fighting sim"){
// open the game
openfightingsim();
}
else if (commandcentervar === "open.game < Extermination" || commandcentervar === "open.game < extermination"){
// open the game
openextermination();
}
else if (commandcentervar === "open.game < Teacher.exe" || commandcentervar === "open.game < teacher.exe"){
// open the game
openteacherexe();
}
else if (commandcentervar === "open.game < Invest or rest 2" || commandcentervar === "open.game < Invest or Rest 2" || commandcentervar === "open.game < invest or rest 2" || commandcentervar === "open.game < Investorrest2" || commandcentervar === "open.game < investorrest2"){
// open the game
openinvestorrest2();
}
else if (commandcentervar === "open.game < Walking Simulator" || commandcentervar === "open.game < walking simulator" || commandcentervar === "open.game < Walkingsimulator" || commandcentervar === "open.game < walkingsimulator" || commandcentervar === "open.game < Walkingsim"){
// open the game
openwalkingsim();
}
else{
alert("Unsuccesfull!");
}
}






